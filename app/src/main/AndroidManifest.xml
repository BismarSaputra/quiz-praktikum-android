<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ayobantu">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_tree"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tree_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ayobantu">
        <activity android:name=".ui.food.FoodActivity"></activity>
        <activity
            android:name=".ui.auth.AuthActivity"
            android:theme="@style/Theme.Ayobantu.Auth">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.MainActivity" />
    </application>

</manifest>